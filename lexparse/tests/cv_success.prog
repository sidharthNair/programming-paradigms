project test
    interface I {
        int getp(int i);
        int getn(int i);
        int getSum(int i);
    }

    class Table implements I {
        int pos[], neg[];
        {
            void  putp(int a, int idx) { this.pos[idx]=a; }
            void  putn (int a, int idx) { this.neg[idx]=a; }
            int  getp (int idx) { return pos[idx]; }
            int  getn (int idx) { return neg[idx]; }
            int  getSum (int i)
                int ret;
            {
                ret = this.pos[i] + this.neg[i];
                return ret;
            }
        }
    }

    Table val;
{
    void entry()
    {
        val.putp (1, 0);
        val.putn (-2, 0);
        print (val.getSum(0));
    }
}
